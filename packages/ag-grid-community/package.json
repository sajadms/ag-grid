{
  "name": "ag-grid-community",
  "version": "21.2.2",
  "description": "Advanced Data Grid / Data Table supporting Javascript / React / AngularJS / Web Components",
  "main": "./main.js",
  "module": "./dist/es6/main.js",
  "scripts": {
    "lint": "tslint -p tsconfig.json src/ts/**/*.ts",
    "lint-fix": "tslint -p tsconfig.json -c tslint-fix.json --fix src/ts/**/*.ts",
    "watchWebpack": "webpack --watch",
    "precheck": "tsc -p tsconfig-precheck.json",
    "build": "npm run precheck && npx gulp tsc-scss-clean && npm run webpack-updater",
    "package": "npx gulp webpack-all-no-clean && npm run rollup",
    "webpack-updater": "node webpackUpdater.js",
    "rollup": "node rollup/build.js",
    "test": "npm run validate-es5",
    "test:e2e": "npm run webdriver-start && npm run ci || npm run webdriver-kill",
    "e2e": "protractor protractor.conf.js",
    "webdriver-start": "npm run update-webdriver && webdriver-manager start --standalone --detach",
    "webdriver-stop": "webdriver-manager shutdown",
    "webdriver-kill": "kill $(ps -ef | grep webdriver | grep -v grep | awk '{ print $2 }')",
    "update-webdriver": "webdriver-manager update --gecko false",
    "start-server": "http-server .",
    "ci": "start-server-and-test start-server http://localhost:8080 e2e",
    "validate-es5": "eslint dist/lib"
  },
  "typings": "./main.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/ag-grid/ag-grid.git"
  },
  "keywords": [
    "web-components",
    "grid",
    "data",
    "table",
    "angular",
    "angular-component",
    "react",
    "react-component",
    "angularjs",
    "reactjs"
  ],
  "jspm": {
    "main": "jspm-main"
  },
  "author": "Niall Crosby <niall.crosby@ag-grid.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ag-grid/ag-grid/issues"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10",
    "not ie_mob <= 11",
    "not blackberry > 0"
  ],
  "homepage": "http://www.ag-grid.com/",
  "devDependencies": {
    "autoprefixer": "^9.6.0",
    "cache-loader": "^4.1.0",
    "css-loader": "^3.2.0",
    "cssnano": "^4.1.0",
    "del": "^5.1.0",
    "eslint": "^6.3.0",
    "eslint-plugin-compat": "^3.3.0",
    "eslint-plugin-ie11": "^1.0.0",
    "file-loader": "^4.2.0",
    "glob": "^7.1.4",
    "gulp": "^4.0.0",
    "gulp-clean": "^0.4.0",
    "gulp-cssnano": "^2.1.3",
    "gulp-filter": "^6.0.0",
    "gulp-flatmap": "^1.0.0",
    "gulp-header": "^2.0.0",
    "gulp-jasmine": "^4.0.0",
    "gulp-postcss": "^8.0.0",
    "gulp-rename": "^1.4.0",
    "gulp-replace": "^1.0.0",
    "gulp-sass": "^4.0.0",
    "gulp-sourcemaps": "^2.6.0",
    "gulp-stylus": "^2.7.0",
    "gulp-tsd": "^0.1.0",
    "gulp-typescript": "^5.0.0",
    "gulp-uglify": "^3.0.0",
    "http-server": "^0.11.0",
    "image-webpack-loader": "^6.0.0",
    "jasmine-core": "^3.4.0",
    "json-stable-stringify": "^1.0.0",
    "merge2": "^1.2.0",
    "mini-css-extract-plugin": "^0.8.0",
    "nib": "^1.1.0",
    "node-sass": "^4.12.0",
    "postcss-loader": "^3.0.0",
    "postcss-scss": "^2.0.0",
    "postcss-svgo": "^4.0.0",
    "protractor": "^5.4.0",
    "rollup": "^1.21.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "sass-loader": "^8.0.0",
    "start-server-and-test": "^1.10.0",
    "style-loader": "^1.0.0",
    "stylus": "^0.54.0",
    "terser-webpack-plugin": "^2.0.1",
    "tslint": "^5.19.0",
    "typescript": "~3.6.0",
    "url-loader": "^2.1.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-named": "^1.1.0",
    "webpack": "^4.39.0",
    "webpack-stream": "5.0.0"
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 180,
    "tabWidth": 4
  }
}
